[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
[pull]
	rebase = true
[init]
	defaultBranch = main
[user]
	name = Aidan Starke
	email = aidan@starkedev.net
	signingkey = {{ .git.signingkey }}
[commit]
	gpgsign = true
[tag]
	gpgsign = true
[gpg]
	program = /usr/bin/gpg
[credential]
	helper = /usr/lib/git-core/git-credential-libsecret
